<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>snapshotSafariBookmarks</string>
	<key>Nice</key>
	<integer>20</integer>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/osascript</string>
		<string>-e</string>
		<string>tell application "Finder" to duplicate file "Bookmarks.plist" of folder "Safari" of folder "Library" of home</string>
	</array>
	<key>ThrottleInterval</key>
	<integer>1</integer>
	<key>WatchPaths</key>
	<array>
		<string>/Users/jk/Library/Safari/Bookmarks.plist</string>
	</array>
</dict>
</plist>
